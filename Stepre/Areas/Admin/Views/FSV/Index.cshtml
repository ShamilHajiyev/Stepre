@{
    FreeShippingValue fsv = Model;
}

<div class="row">
    <div class="col-md-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Free shipping value</h4>
                <p>
                    @fsv.IsExist
                </p>
                <p>
                    @fsv.MinimalAmount
                </p>
                <p>
                    <a class="btn btn-success" asp-action="update">Update</a>
                </p>
            </div>
        </div>
    </div>
</div>